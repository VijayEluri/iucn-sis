<module rename-to="SIS">

	<!-- Inherit libraries -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name='com.google.gwt.user.RichText'/>
	<inherits name='com.google.gwt.xml.XML' />
	
	<inherits name='com.solertium.lwxml.LWXML' />
	<inherits name='com.solertium.util.gwt.Util' />
	<inherits name='com.solertium.util.Portable' />
	<inherits name='com.solertium.util.extjs.ExtGWTUtils' />
	<inherits name='com.solertium.util.extjs.login.ExtGWTUtils' />
	
	<inherits name='com.extjs.gwt.Resources' />
	<inherits name='com.extjs.gxt.ui.GXT'/>
	<inherits name='com.extjs.gxt.themes.Themes' />
	
	<inherits name='ext.ux.wizard.WizardWindow'/>
	<inherits name='ext.ux.pwd.PasswordField'/>

	<!-- Inherit independent SIS modules -->
	<inherits name='org.iucn.sis.Shared' />
	<inherits name='org.iucn.sis.References' />
	<inherits name='org.iucn.sis.Users' />
	<inherits name='org.iucn.sis.Integrity' />
	<inherits name='org.iucn.sis.BirdLife' />

	<!-- Specify the app entry point class. -->
	<entry-point class="org.iucn.sis.client.container.OfflineSimpleSISClient"/>
	
	<source path='client'/>
	<source path='shared'/> 
	
</module>

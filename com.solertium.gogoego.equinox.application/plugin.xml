<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="application"
         name="GoGoEgo"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="com.solertium.gogoego.equinox.application.Application"/>
      </application>
   </extension>
   <extension
         id="product"
         name="GoGoEgo"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.solertium.gogoego.equinox.application.application"
            name="GoGoEgo">
         <property
               name="appName"
               value="GoGoEgo">
         </property>
      </product>
   </extension>

</plugin>

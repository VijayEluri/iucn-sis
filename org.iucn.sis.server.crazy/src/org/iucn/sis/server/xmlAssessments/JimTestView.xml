<view id="JimView" title="Jim's Test View" xsi:noNamespaceSchemaLocation="views.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<pages>
		<page title="Distribution and Range" id="Distribution">
			<pageOrganizations>
				<organization title="Geographic Range Information">
					<field id="RangeDocumentation"></field>
				</organization>
				<organization title="Area of Occupancy">
					<composite>
						<field id="AOO"></field>
						<field id="AOOContinuingDecline"></field>
					</composite>
					<composite>
						<field id="AOOExtremeFluctuation"></field>
						<field id="AreaRestricted"></field>
					</composite>		
				</organization>
				<organization title="Extent of Occurrence">
				  <composite>
						<field id="EOO"></field>
						<field id="EOOContinuingDecline"></field>
					</composite>
					<composite>
						<field id="EOOExtremeFluctuation"></field>
					</composite>
			  </organization>
		    <organization title="Elevation/Depth">
          <composite>
					<field id="ElevationUpper"></field>
					<field id="ElevationLower"></field>
					</composite>
					<composite>
					<field id="DepthUpper"></field>
					<field id="DepthLower"></field>
					</composite>
				</organization>
				<organization title="Map Status">
					<field id="MapStatus"></field>
				</organization>
				<organization title="Depth Zones">
					<field id="DepthZone"></field>
				</organization>
			  <organization title="Biogeographic Realms">
					<field id="BiogeographicRealm"></field>
				</organization>
			</pageOrganizations>
	  </page>
    <page title="Population" id="Population">
			<pageOrganizations>
        <organization title="Population Documentation">
          <field id="PopulationDocumentation"></field>
        </organization>
        <organization title="Population Size Information">
          <field id="PopulationSize"></field>
        </organization>
        <organization title="Population Reduction - Past 10 Years or 3 Generations">
          <composite>
            <field id="PopulationReductionPast"></field>
            <field id="PopulationReductionPastBasis"></field>
          </composite>
          <composite>
            <field id="PopulationReductionPastReversible"></field>
            <field id="PopulationReductionPastUnderstood"></field>
          </composite>
          <composite>
            <field id="PopulationReductionPastCeased"></field>
          </composite>
          
        </organization>
        <organization title="Population Reduction - Within Next 10 Years or 3 Generations">
          <composite>
            <field id="PopulationReductionFuture"></field>
            <field id="PopulationReductionFutureBasis"></field>
          </composite>
          <composite>
            <field id="PopulationReductionFutureReversible"></field>
            <field id="PopulationReductionFutureUnderstood"></field>
          </composite>
          <composite>
            <field id="PopulationReductionFutureCeased"></field>
          </composite>  
        </organization>
        <organization title="Population Reduction - Any 10 Year or 3 Generation Period">
          <composite>
            <field id="PopulationReductionPastandFuture"></field>
            <field id="PopulationReductionPastandFutureBasis"></field>
          </composite>
          <composite>
            <field id="PopulationReductionPastandFutureReversible"></field>
            <field id="PopulationReductionPastandFutureUnderstood"></field>
          </composite>
          <composite>
            <field id="PopulationReductionPastandFutureCeased"></field>
          </composite>
        </organization>
        <organization title="Continuing Decline Information">
          <composite>
          <field id="PopulationContinuingDecline"></field>
          </composite>
</organization>
     </pageOrganizations> 
   </page>            
		<page title="Ecosystem Services" id="EcosystemServices">
<!-- AAH note: this page added by AAH -->
			<pageOrganizations>
				<organization title="Ecosystem Services Provided by the Species">
					<composite>
						<field id="EcosystemServicesInsufficientInfo"></field>
						<field id="EcosystemServicesProvidesNone"></field>
					</composite>
<!-- AAH note: need Ecosystem service/Score from 1 to 5/Geographic range over which benefits are felt -->
				</organization>
			</pageOrganizations>
		</page>
		

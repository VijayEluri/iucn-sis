CREATE TABLE ResearchSubfield (id int(10) auto_increment, name varchar(255), type varchar(255), number_allowed varchar(255))
INSERT INTO ResearchSubfield VALUES ResearchSchemeLookup, fk_primitive_field, 1
#----------------------------------------
CREATE TABLE RiversStructure0_aLookup(id int(10) auto_increment, name varchar(255), value varchar(255)
INSERT INTO RiversStructure0_aLookup VALUES 0, Year Round
INSERT INTO RiversStructure0_aLookup VALUES 1, Breeding Season Only
INSERT INTO RiversStructure0_aLookup VALUES 2, Non-breeding Season Only
INSERT INTO RiversStructure0_aLookup VALUES 3, Possibly Extinct
INSERT INTO RiversStructure0_aLookup VALUES 4, Extinct
INSERT INTO RiversStructure0_aLookup VALUES 5, Uncertain
CREATE TABLE RiversStructureOrigin_Lookup(id int(10) auto_increment, name varchar(255), value varchar(255)
INSERT INTO RiversStructureOrigin_Lookup VALUES 0, Native
INSERT INTO RiversStructureOrigin_Lookup VALUES 1, Introduced
INSERT INTO RiversStructureOrigin_Lookup VALUES 2, Reintroduced
INSERT INTO RiversStructureOrigin_Lookup VALUES 3, Vagrant
INSERT INTO RiversStructureOrigin_Lookup VALUES 4, Uncertain
CREATE TABLE Rivers(id int(10) auto_increment, name varchar(255), type varchar(255), number_allowed varchar(255))
INSERT INTO Rivers VALUES RiversSubfield, field, 0..
CREATE TABLE RiversSubfield (id int(10) auto_increment, name varchar(255), type varchar(255), number_allowed varchar(255))
INSERT INTO RiversSubfield VALUES RiversSchemeLookup, fk_primitive_field, 1
INSERT INTO RiversSubfield VALUES a, fk_primitive_field, 0..1
INSERT INTO RiversSubfield VALUES N/A, boolean_primitive_field, 0..1
INSERT INTO RiversSubfield VALUES N/A, fk_primitive_field, 0..1

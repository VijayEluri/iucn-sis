CREATE TABLE Threat_timingLookup(id int(10) auto_increment, name varchar(255), value varchar(255)
INSERT INTO Threat_timingLookup VALUES 0, Past, Unlikely to Return
INSERT INTO Threat_timingLookup VALUES 1, Ongoing
INSERT INTO Threat_timingLookup VALUES 2, Future
INSERT INTO Threat_timingLookup VALUES 3, Unknown
INSERT INTO Threat_timingLookup VALUES 4, Past, Likely to Return
CREATE TABLE Threat_scopeLookup(id int(10) auto_increment, name varchar(255), value varchar(255)
INSERT INTO Threat_scopeLookup VALUES 0, Whole (>90%)
INSERT INTO Threat_scopeLookup VALUES 1, Majority (50-90%)
INSERT INTO Threat_scopeLookup VALUES 2, Minority (<50%)
INSERT INTO Threat_scopeLookup VALUES 3, Unknown
CREATE TABLE Threat_severityLookup(id int(10) auto_increment, name varchar(255), value varchar(255)
INSERT INTO Threat_severityLookup VALUES 0, Very Rapid Declines
INSERT INTO Threat_severityLookup VALUES 1, Rapid Declines
INSERT INTO Threat_severityLookup VALUES 2, Slow, Significant Declines
INSERT INTO Threat_severityLookup VALUES 3, Causing/Could cause fluctuations
INSERT INTO Threat_severityLookup VALUES 4, Negligible declines
INSERT INTO Threat_severityLookup VALUES 5, No decline
INSERT INTO Threat_severityLookup VALUES 6, Unknown
CREATE TABLE Threats(id int(10) auto_increment, name varchar(255), type varchar(255), number_allowed varchar(255))
INSERT INTO Threats VALUES ThreatsSubfield, field, 0..
CREATE TABLE ThreatsSubfield (id int(10) auto_increment, name varchar(255), type varchar(255), number_allowed varchar(255))
INSERT INTO ThreatsSubfield VALUES ThreatsSchemeLookup, fk_primitive_field, 1
INSERT INTO ThreatsSubfield VALUES timing, fk_primitive_field, 0..1
INSERT INTO ThreatsSubfield VALUES scope, fk_primitive_field, 0..1
INSERT INTO ThreatsSubfield VALUES severity, fk_primitive_field, 0..1
INSERT INTO ThreatsSubfield VALUES score, integer_primitive_field, 0..1
INSERT INTO ThreatsSubfield VALUES Stresses, StressesSubfield, 0..
CREATE TABLE StressesSubfield (id int(10) auto_increment, name varchar(255), type varchar(255), number_allowed varchar(255))
INSERT INTO StressesSubfield VALUES stress, fk_primitive_field, 0..
INSERT INTO ThreatsSubfield VALUES c, text_primitive_field, 0..1
